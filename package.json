{
  "name": "api-sales-jke",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias && npx prisma generate",
    "start": "node dist/index.js",
    "start:prod": "npx prisma migrate deploy && npm run prisma:seed && node dist/index.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init --skip-generate",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx tsx prisma/seed.ts",
    "dev": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed && tsx watch src/index.ts",
    "dev:native": "node --env-file=.env -r tsx src/index.ts",
    "docker:prod": "docker-compose up --build api",
    "docker:dev": "cross-env NODE_ENV=development BUILD_TARGET=development docker-compose --profile local-services up --build",
    "docker:down": "docker-compose --profile local-services down",
    "docker:clean": "docker-compose --profile local-services down -v --remove-orphans",
    "docker:reset": "npm run docker:clean && npm run docker:dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonico22/api-sales-jke.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jonico22/api-sales-jke/issues"
  },
  "homepage": "https://github.com/jonico22/api-sales-jke#readme",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/hpp": "^0.2.7",
    "@types/node": "^25.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "cross-env": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "prisma": "^6.19.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "@prisma/client": "^6.19.1",
    "@scalar/express-api-reference": "^0.8.30",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "pino": "^10.1.0",
    "rate-limit-redis": "^4.3.1",
    "redis": "^5.10.0",
    "swagger-jsdoc": "^6.2.8",
    "zod": "^4.3.4"
  }
}